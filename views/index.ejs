<html lang="en">
<head>
  <%- include ('partials/head.ejs') %>
  <title>Index</title>
</head>
<body>
  <ul>
    <% for(let i = 0; i < paintings.length; i++){ %>
      <li>

          <%=paintings[i].title%> <br>
          <%=paintings[i].author%> <br>
          <%=paintings[i].year%> <br>
          <a href="/paintings/<%=paintings[i].id%>">
             <img src="<%=paintings[i].imageURL%>" alt=""> 
          </a>
          
        
          <form action="/paintings/<%=paintings[i].id%>?_method=DELETE" method="POST">
              <input type="submit" value="DELETE"/>
          </form>
          <a href="/paintings/<%=paintings[i].id%>/edit">Edit</a>
      </li>
  <% } %>
  </ul>
</body>
</html>

    